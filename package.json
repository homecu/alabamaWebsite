{
  "name": "united-members-website",
  "private": true,
  "description": "Gatsby project for United Members",
  "version": "1.0.0",
  "author": "Daniel Salgado",
  "scripts": {
    "start": "npm run develop",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "constructor": "gatsby clean && gatsby build && gatsby serve",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "format": "prettier --write \"src/**/*.{js,jsx,json,md,yml,css}\"",
    "lint": "eslint --fix \"src/**/*.{js,jsx}\""
  },
  "dependencies": {
    "@gatsbyjs/reach-router": "^1.0.0",
    "@hellobuild/blossom-websites-core-package": "latest",
    "@reach/router": "^1.3.4",
    "classnames": "^2.2.6",
    "gatsby": "^2.26.1",
    "gatsby-link": "^3.14.0",
    "gatsby-plugin-manifest": "^2.6.1",
    "gatsby-plugin-postcss": "^3.4.0",
    "gatsby-plugin-react-helmet": "^3.4.0",
    "gatsby-plugin-robots-txt": "^1.5.3",
    "gatsby-plugin-simple-analytics": "^1.0.3",
    "gatsby-plugin-sitemap": "^2.9.0",
    "gatsby-source-filesystem": "^2.5.0",
    "hoist-non-react-statics": "^3.3.2",
    "lodash": "^4.17.21",
    "postcss": "^8.2.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-error-boundary": "^3.1.0",
    "react-is": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.1",
    "tailwindcss": "^2.0.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.18.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "sass": "^1.32.7"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "0BSD",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "src/**/*.{json,md,yml,css}": [
      "prettier --write",
      "git add"
    ]
  }
}
